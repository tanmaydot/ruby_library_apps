<div class="container mt-5">
  <% if can? :manage, Rak %>
    <h1 class="text-center text-warning">Create Rack</h1>
    <div class="row justify-content-center">
      <div class="col-md-6">
        <%= render partial: 'shared/rak_form', locals: { rak: Rak.new } %>
      </div>
    </div>
  <% else %>
    <%= render partial: '/shared/not_authorized' %>
  <% end %>
</div>
